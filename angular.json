{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "voice-list": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/voice-list",
      "sourceRoot": "apps/voice-list/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/voice-list",
            "index": "apps/voice-list/src/index.html",
            "main": "apps/voice-list/src/main.ts",
            "polyfills": "apps/voice-list/src/polyfills.ts",
            "tsConfig": "apps/voice-list/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/voice-list/src/favicon.ico",
              "apps/voice-list/src/assets",
              "apps/voice-list/src/manifest.webmanifest"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/voice-list/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/voice-list/src/environments/environment.ts",
                  "with": "apps/voice-list/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "voice-list:build",
            "proxyConfig": "apps/voice-list/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "voice-list:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "voice-list:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/voice-list/src/test.ts",
            "polyfills": "apps/voice-list/src/polyfills.ts",
            "tsConfig": "apps/voice-list/tsconfig.spec.json",
            "karmaConfig": "apps/voice-list/karma.conf.js",
            "assets": [
              "apps/voice-list/src/favicon.ico",
              "apps/voice-list/src/assets",
              "apps/voice-list/src/manifest.webmanifest"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/voice-list/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/voice-list/tsconfig.app.json",
              "apps/voice-list/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
