    "nativescript-tt-pothole-report": {
      "root": "apps/.//nativescript-tt-pothole-report/",
      "sourceRoot": "apps/.//nativescript-tt-pothole-report/src",
      "projectType": "application",
      "prefix": "qarun-qb",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn start.nativescript.tt-pothole-report.preview"
              }
            ]
          }
        },
        "ios": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "ns debug ios --no-hmr"
              }
            ],
            "cwd": "apps/.//nativescript-tt-pothole-report",
            "parallel": false
          }
        },
        "android": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "ns debug android --no-hmr"
              }
            ],
            "cwd": "apps/.//nativescript-tt-pothole-report",
            "parallel": false
          }
        },
        "clean": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx rimraf -- hooks node_modules platforms package-lock.json"
              },
              {
                "command": "npm i"
              },
              {
                "command": "npx rimraf -- package-lock.json"
              }
            ],
            "cwd": "apps/.//nativescript-tt-pothole-report",
            "parallel": false
          }
        }
      }
    }
